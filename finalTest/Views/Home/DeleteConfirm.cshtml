
@model Entertainer

<h2>Delete Entertainer</h2>
<h3>Are you sure you want to delete this entertainer: @Model.EntStageName?</h3>

<form asp-action="DeleteConfirm" method="post">
    <input type="hidden" asp-for="EntertainerId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
